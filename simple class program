#include<iostream>

using namespace std;

class book
{
    public:
        int a;
        char b;
};
int main()
{
    book isis;
    isis.a=66;
    isis.b='c';
    cout<<isis.a<<endl<<isis.b;
}


/* simple constructor program   */

#include <iostream>

using namespace std;

class book
{
    public:
        int a;
        book(void);
};
book::book()
{
    a=56;
}
int main() {
   
   book book;
   cout<<book.a;
}
